#include <stdlib.h>
#include <time.h>
/* more headers goes there */

/**
 * main - programs entry point
 * Description: Outputs the last digit of a randomly generated number
 * Return: 0 if succesfull 1 of something goes wrong
 */
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;

	int last_digit = n % 10;

        string comp_6
		printf("Last digit of %d is %d and is %s 6 and not 0", n, last_digit);
	return (0);
}
