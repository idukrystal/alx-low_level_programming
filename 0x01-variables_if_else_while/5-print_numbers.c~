 (12 sloc)  234 Bytes
#include <stdlib.h>
#include <time.h>
/* more headers goes there */

 /**
  * main - entry point of program
  * Description: prints the alphabets in lower case
  * Return: 0 if succesful 1 if not
  */
int main(void)
{
	for (char c = '0'; c <= '9'; c++)
		putchar(c);

	putchar('\n');

	return (0);
}
